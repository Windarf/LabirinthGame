<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="519.0" prefWidth="470.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="game.gui.MenuController">
   <children>
      <Button fx:id="startButton" focusTraversable="false" layoutX="195.0" layoutY="260.0" mnemonicParsing="false" onAction="#startGame" prefHeight="50.0" prefWidth="80.0" text="Start" />
      <Button fx:id="exitButton" focusTraversable="false" layoutX="195.0" layoutY="391.0" mnemonicParsing="false" onAction="#handleExitButton" prefHeight="50.0" prefWidth="80.0" text="Exit" />
      <Button fx:id="rulesButton" focusTraversable="false" layoutX="195.0" layoutY="325.0" mnemonicParsing="false" onAction="#handleRulesButton" prefHeight="50.0" prefWidth="80.0" text="Rules" />
      <Label layoutX="119.0" layoutY="56.0" text="Labyrinth Game!">
         <font>
            <Font size="32.0" />
         </font>
      </Label>
      <TextField fx:id="usernameTextField" focusTraversable="false" layoutX="161.0" layoutY="217.0" promptText="Username" />
   </children>
</AnchorPane>
